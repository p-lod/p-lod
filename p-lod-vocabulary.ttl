@prefix p-lod: <http://digitalhumanities.umass.edu/p-lod/identifiers/> .


@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

p-lod:entity a owl:Class ;
    rdfs:label "P-LOD Entity"  .

p-lod:insula a owl:Class ;
    rdfs:subClassOf p-lod:entity ;
    rdfs:label "Pompeian Insula" .

p-lod:region a owl:Class ;
    rdfs:label "Pompeian Region" ;
    dfs:subClassOf p-lod:entity .
    
p-lod:property a owl:Class ;
    rdfs:label "Pompeian Property" ;
    dfs:subClassOf p-lod:entity .
    
p-lod:room a owl:Class ;
    rdfs:label "Room" ;
   dfs:subClassOf p-lod:entity .
